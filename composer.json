{
  "name": "gbuckingham89/laraflash",
  "type": "library",
  "description": "A package for handling flash messages / notifications in Laravel.",
  "keywords": ["Laravel", "Flash", "Messages", "Notifications", "PHP"],
  "homepage": "https://github.com/gbuckingham89/laraflash",
  "license": "MIT",
  "authors": [
    {
      "name": "George Buckingham",
      "email": "george@georgebuckingham.com",
      "homepage": "https://www.georgebuckingham.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/gbuckingham89/laraflash/issues",
    "source": "https://github.com/gbuckingham89/laraflash"
  },
  "require": {
    "php": "7.4.*|8.0.*|8.1.*",
    "illuminate/testing": "^7.0|^8.0|^9.0",
    "illuminate/session": "^7.0|^8.0|^9.0",
    "illuminate/support": "^7.0|^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Gbuckingham89\\Laraflash\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gbuckingham89\\Laraflash\\ServiceProvider"
      ],
      "aliases": {
        "Laraflash": "Gbuckingham89\\Laraflash\\Facade"
      }
    }
  }
}
